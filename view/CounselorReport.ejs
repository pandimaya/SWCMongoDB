  </header><!-- End Header -->

  <main>
    <div class="container">
      <h1>Report</h1>
      <form action="/submit_data" method="POST">
        <div class="mb-3">
          <label for="fname" class="form-label">First Name:</label>
          <input type="text" class="form-control" id="fname" name="fname">
        </div>
        <div class="mb-3">
          <label for="lname" class="form-label">Last Name:</label>
          <input type="text" class="form-control" id="lname" name="lname">
        </div>
        <div class="mb-3">
          <label for="email" class="form-label">Email:</label>
          <input type="text" class="form-control" id="email" name="email" oninput="suggestEmails()">
      </div>
      <div id="emailSuggestions"></div>
        <div class="mb-3">
          <label for="progcode" class="form-label">Progcode:</label>
          <input type="text" class="form-control" id="progcode" name="progcode">
        </div>
        <div class="mb-3">
            <label for="schoolYear" class="form-label">School Year:</label>
            <input type="text" class="form-control" id="schoolYear" name="schoolYear">
          </div>
        <div class="mb-3">
            <label for="department" class="form-label">Department:</label>
            <input type="text" class="form-control" id="department" name="department">
          </div>
        <div class="mb-3">
          <label for="typeOfService" class="form-label">Type of Service:</label>
          <input type="text" class="form-control" id="typeOfService" name="typeOfService">
        </div>
        <div class="mb-3">
          <label for="categoryType" class="form-label">Category Type</label>
          <input type="text" class="form-control" id="categoryType" name="categoryType">
        </div>
        <div class="mb-3">
          <label for="natureOfConcern" class="form-label">Nature of Concern:</label>
          <input type="text" class="form-control" id="natureOfConcern" name="natureOfConcern">
        </div>
        <div class="mb-3">
          <label for="counselingClient" class="form-label">Counseling Client:</label>
          <input type="text" class="form-control" id="counselingClient" name="counselingClient">
        </div>
        <div class="mb-3">
          <label for="sessionName" class="form-label">Session Name:</label>
          <input type="text" class="form-control" id="sessionName" name="sessionName">
        </div>
        <div class="mb-3">
          <label for="note" class="form-label">Note:</label>
          <textarea class="form-control" id="note" name="note" rows="5"></textarea>
        </div>
        <button type="button"  class="btn btn-primary"onclick="insertRecord()">Submit
        </button>
      </form>
    </div>
  </main>
  
  

